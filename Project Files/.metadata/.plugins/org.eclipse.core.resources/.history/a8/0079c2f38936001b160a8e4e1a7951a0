package MDA_EFSM;

public class S5 extends State {
	public S5(MDA_EFSM m, OP p) { super(m, p); }
	
	public void Pump() {
		// self-looping state
		if(m.s == m.LS[5]) {
			p.PumpGasUnit();
			p.GasPumpedMsg();
		}
	}
	
	public void StopPump() {
		if (m.s == m.LS[5]) {
			p.PrintReceipt();
			m.s = m.LS[1]
		}
	}
}
